{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "quotes": [
            "warn",
            "single",
            {"avoidEscape": true, "allowTemplateLiterals": true}
        ],
        "no-magic-numbers": ["warn", {"ignore": [0, 1]}]
    }
}
